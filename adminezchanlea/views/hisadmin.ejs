<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="col-12 col-xl-8 mb-4 mb-xl-0">
                <h3 class="font-weight-bold">Trang lịch sử admin
                </h3>

            </div>

        </div>
    </div>
</div>

<div class="row mt-2">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <p class="card-title mb-0">Lịch sử admin</p>
                <div class="table-responsive">
                    <table class=" table table-striped table-borderless">
                        <thead>
                            <tr>
                                <th>Nội dung</th>
                                <th>Thời gian</th>
                              
                            </tr>
                        </thead>
                        <tbody>




                            <% if (lichsu.length<=0) { %>
                                <tr class="text-center">
                                    <td colspan="2">Không có đơn</td>
                                </tr>
                                <% } %>
                                    <% for (var i=0; i < lichsu.length; i++) { %>
                                        <tr>
                                         
                                            <td scope="col">
                                                <%= lichsu[i].noidung %>
                                            </td>

                                            <td scope="col">
                                                <%= new Date(Date.parse(lichsu[i].date)).toLocaleString()%>
                                            </td>

                                            

                                           
                                        </tr>


                                        <% } %>







                        </tbody>
                    </table>

                </div>
                
                <% if (pages> 0) { %>
                    <nav aria-label="...">
                        <ul class="pagination mt-2">
                            <% if (current==1) { %>
                                <li class="page-item disabled"><a class="page-link">Đầu</a></li>
                                <% } else { %>
                                    <li><a class="page-link" href="/hisadmin?page=1">Đầu</a></li>
                                    <% } %>
                                        <% var i=(Number(current)> 2 ? Number(current) - 2 : 1) %>
                                            <% if (i !==1) { %>
                                                <li class="disabled"><a class="page-link">...</a></li>
                                                <% } %>
                                                    <% for (; i <=(Number(current) + 2) && i <=pages; i++) { %>
                                                        <% if (i==current) { %>
                                                            <li class="page-item active"><a class="page-link">
                                                                    <%= i %>
                                                                </a></li>
                                                            <% } else { %>
                                                                <li><a class="page-link"
                                                                        href="/hisadmin?page=<%= i %>">
                                                                        <%= i %>
                                                                    </a></li>
                                                                <% } %>
                                                                    <% if (i==Number(current) + 2 && i < pages) { %>
                                                                        <li class="page-item disabled"><a
                                                                                class="page-link">...</a></li>
                                                                        <% } %>
                                                                            <% } %>
                                                                                <% if (current==pages) { %>
                                                                                    <li class="page-item disabled">
                                                                                        <a
                                                                                            class="page-link">Cuối</a>
                                                                                    </li>
                                                                                    <% } else { %>
                                                                                        <li><a class="page-link"
                                                                                                href="/hisadmin?page=<%= pages %>">Cuối</a>
                                                                                        </li>
                                                                                        <% } %>
                        </ul>
                    </nav>
                    <% } %>
            </div>
        </div>
    </div>

</div>
